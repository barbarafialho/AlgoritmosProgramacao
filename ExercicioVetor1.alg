algoritmo "ExercicioVetor1"
// Aluna: Barbara Amanda Fialho de Souza
var
numeros:vetor[1..10] de inteiro
cont: inteiro
inicio

para cont <-1 ate 10 faca
     escreva("Digite um número inteiro para a posição ", cont, ": ")
     leia(numeros[cont])
fimpara

para cont <-1 ate 10 faca
     escreval("Número na posição ", cont, ": ", numeros[cont])
fimpara

fimalgoritmo
